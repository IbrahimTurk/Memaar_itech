<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

      <record id="category_res_company_form_view_inherit" model="ir.ui.view">
        <field name="name">category.res.company.product.form.inherit</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
         <xpath expr="//page[1]" position="after">
            <page name="additional" string="Additional Setting">
             <group> 
               <field name="category_search" string="Category Search"/>
             </group> 
             </page>
          </xpath>
        </field>
      </record>
    </data>

</odoo>




